            </e:Skin>
        </e:Button>
        <e:Button id="btnStore" x="125" verticalCenter="0">
            <e:Skin>
                <e:Image source="btn-store"></e:Image>
                <e:Label text="商城" y="120" bold="true" textColor="0x000000" horizontalCenter="0"></e:Label>
            </e:Skin>
        </e:Button>
        <e:Button id="btnShare" verticalCenter="0" right="125">
            <e:Skin>
                <e:Image source="btn-share"></e:Image>
                <e:Label text="分享" y="120" bold="true" textColor="0x000000" horizontalCenter="0"></e:Label>
            </e:Skin>
        </e:Button>
        <e:Button id="btnSetting" verticalCenter="0" right="0">
            <e:Skin>
                <e:Image source="btn-settings"></e:Image>
                <e:Label text="设置" y="120" bold="true" textColor="0x000000" horizontalCenter="0"></e:Label>
            </e:Skin>
        </e:Button>
    </e:Group>
</e:Skin>
